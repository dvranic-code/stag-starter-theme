{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "color": {
            "palette": [
                {
                    "name": "Blue",
                    "slug": "blue",
                    "color": "#00308E"
                },
                {
                    "name": "Violet",
                    "slug": "violet",
                    "color": "#8C01CB"
                },
                {
                    "name": "Text",
                    "slug": "text",
                    "color": "#575757"
                },
                {
                    "name": "Black",
                    "slug": "black",
                    "color": "#000000"
                },
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#ffffff"
                }
            ],
            "defaultPalette": false
        },
        "custom": {
        },
        "layout": {
            "contentSize": "1140px",
            "wideSize": "1140px"
        },
        "spacing": {
            "spacingScale": {
				"steps": 0
			},
            "units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
        },
        "typography": {
            "fluid": true,
            "fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/Poppins-Regular.ttf"
							]
						},
                        {
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/Poppins-Medium.ttf"
							]
						},
                        {
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/Poppins-SemiBold.ttf"
							]
						}
					],
					"fontFamily": "\"Poppins\", sans-serif",
					"name": "Poppins",
					"slug": "poppins"
				},
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
            "fontSizes": [
                {
                    "fluid": {
						"min": "12px",
						"max": "16px"
					},
                    "size": "16px",
                    "slug": "minimum"
                },
                {
                    "fluid": {
						"min": "16px",
						"max": "20px"
					},
                    "size": "20px",
                    "slug": "small"
                },
                {
                    "fluid": {
						"min": "26px",
						"max": "54px"
					},
                    "size": "54px",
                    "slug": "medium"
                },
                {
                    "fluid": {
						"min": "30px",
						"max": "60px"
					},
                    "name": "Big",
                    "size": "60px",
                    "slug": "big"
                }
            ]
        },
        "useRootPaddingAwareAlignments": true
    },
    "styles": {
        "spacing": {
			"blockGap": "30px"
		},
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--poppins)",
			"fontSize": "var(--wp--preset--font-size--small )",
			"lineHeight": "1.5"
        }
    }
}