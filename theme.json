{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "border": {
            "radius": true,
            "color": true,
            "style": true,
            "width": true
        },
        "color": {
            "link": true,
            "custom": true,
            "customDuotone": true,
            "customGradient": true,
            "duotone": [],
            "gradients": [],
            "text": true,
            "background": true,
            "defaultGradients": true,
            "defaultPalette": true,
            "palette": [
                {
                    "name": "Blue 1",
                    "slug": "tl-blue-1",
                    "color": "#0A3782"
                },
                {
                    "name": "Blue 2",
                    "slug": "tl-blue-2",
                    "color": "#000484"
                },
                {
                    "name": "Blue 3",
                    "slug": "tl-blue-3",
                    "color": "#0000C9"
                },
                {
                    "slug": "tl-blue-4",
                    "name": "Blue 4",
                    "color": "#003FE2"
                },
                {
                    "name": "Black",
                    "slug": "tl-black",
                    "color": "#0A0A0A"
                },
                {
                    "name": "Grey 1",
                    "slug": "tl-grey-1",
                    "color": "#EBEBF2"
                },
                {
                    "name": "White",
                    "slug": "tl-white",
                    "color": "#FAFAFC"
                },
                {
                    "name": "Default White",
                    "slug": "white",
                    "color": "#FFFFFF"
                },
                {
                    "name": "Grey 2",
                    "slug": "tl-grey-2",
                    "color": "#E6ECFD"
                },
                {   
                    "name": "Grey 3",
                    "slug": "tl-grey-3",
                    "color": "#A1AAB1"
                },
                {
                    "name": "Dark Grey",
                    "slug": "tl-dark-grey",
                    "color": "#858D97"
                }
            ]
        },
        "custom": {
        },
        "layout": {
            "contentSize": "1140px",
            "wideSize": "1140px"
        },
        "spacing": {
            "spacingScale": {
				"steps": 0
			},
            "units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
        },
        "typography": {
            "fluid": true,
            "fontFamilies": [
				{
					"fontFace": [
                        {
							"fontFamily": "PTSans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/PTSans-Bold.ttf"
							]
						},
                        {
							"fontFamily": "PTSans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/PTSans-BoldItalic.ttf"
							]
						},
                        {
							"fontFamily": "PTSans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/PTSans-Italic.ttf"
							]
						},
                        {
							"fontFamily": "PTSans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/PTSans-Regular.ttf"
							]
						}
					],
					"fontFamily": "\"PTSans\", sans-serif",
					"name": "PTSans",
					"slug": "ptsans"
				},
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
            "fontSizes": [
                {
                    "fluid": {
						"min": "12px",
						"max": "16px"
					},
                    "size": "16px",
                    "slug": "minimum"
                },
                {
                    "fluid": {
						"min": "16px",
						"max": "20px"
					},
                    "size": "20px",
                    "slug": "small"
                },
                {
                    "fluid": {
						"min": "26px",
						"max": "54px"
					},
                    "size": "54px",
                    "slug": "medium"
                },
                {
                    "fluid": {
						"min": "30px",
						"max": "60px"
					},
                    "name": "Big",
                    "size": "60px",
                    "slug": "big"
                }
            ]
        },
        "useRootPaddingAwareAlignments": true
    },
    "styles": {
        "spacing": {
			"blockGap": "30px"
		},
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--ptsans)",
			"fontSize": "var(--wp--preset--font-size--small )",
			"lineHeight": "1.5"
        }
    }
}